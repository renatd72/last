config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ff5c0000.usb/usb3/3-1/3-1:1.0'
	option channel '1'
	option band '2g'
	option htmode 'HT20'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

